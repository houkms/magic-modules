<% if output_var == 'd' -%>
d.Set("<%= prop_name -%>", <%= input_var -%>)
<% elsif property.is_a?(Api::Azure::Type::ISO8601Duration) -%>
<%=  output_var -%>["<%= prop_name -%>"] = <%= lines(input_var) -%>
<% elsif property.is_a?(Api::Azure::Type::ISO8601DateTime)  -%>
<%   temp_var = prop_name.camelize(:lower) -%>
if <%= temp_var -%> := <%= input_var -%>; <%= temp_var -%> != nil {
    <%= output_var -%>["<%= prop_name -%>"] = <%= lines("(*#{temp_var}).String()") -%>
}
<% end -%>